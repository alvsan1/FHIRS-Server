 fhir:Condition.subject.type a owl:ObjectProperty ;
	owl:propertyChainAxiom (  fhir:Condition.subject  fhir:Reference.type ) .

 fhir:Condition.subject rdfs:subClassOf [ 
	a owl:Restriction ;
	owl:onProperty  fhir:Condition.subject.type  ;
	owl:someValuesFrom fhir:Patient ;
	owl:someValuesFrom fhir:Group ] . 

 fhir:Condition.encounter.type a owl:ObjectProperty ;
